{
  "name": "tbd",
  "version": "0.0.1",
  "main": "handler.js",
  "author": "Jimmy Cann",
  "scripts": {
    "test": "NODE_ENV=integration-tests mocha --compilers js:babel-core/register ./**/__test__/bootstrap.js ./**/__test__/**/*.test.js",
    "init": "bash ./bin/initialize.sh",
    "deploy:all": "bash ./bin/init-deploy.sh $ENVIRONMENT",
    "deploy:all:test": "bash ./bin/init-deploy.sh test",
    "deploy:all:staging": "bash ./bin/init-deploy.sh staging",
    "deploy:all:live": "bash ./bin/init-deploy.sh live",
    "deploy:f": "bash ./bin/init-deploy.sh $ENVIRONMENT $FUNCTION",
    "deploy:f:test": "bash ./bin/init-deploy.sh test $FUNCTION",
    "deploy:f:staging": "bash ./bin/init-deploy.sh staging $FUNCTION ",
    "deploy:f:live":  "bash ./bin/init-deploy.sh live $FUNCTION ",
    "invoke": "serverless invoke local --env development -f $FUNCTION -p $JSON_PATH",
    "invoke:local": "serverless invoke local --env development -f $FUNCTION -p $JSON_PATH",
    "invoke:test": "serverless invoke --profile nicbot-sls --env test -f $FUNCTION -p $JSON_PATH",
    "invoke:staging": "serverless invoke --profile nicbot-sls --env staging -f $FUNCTION -p $JSON_PATH",
    "invoke:live": "serverless invoke --profile nicbot-sls --env live -f $FUNCTION -p $JSON_PATH",
    "gen-credentials": "AWS_PROFILE=nicbot-kms node ./bin/gen-credentials.js",
    "b": "bash ./bin/build-static-db.sh $ENVIRONMENT"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "dynasty": "^0.2.7"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-preset-latest": "^6.24.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-spies": "^0.7.1",
    "faker": "^4.1.0",
    "mocha": "^3.2.0",
    "nodecredstash": "^1.0.1",
    "serverless-babel-plugin": "^0.1.3",
    "sinon": "^2.1.0",
    "supertest": "^3.0.0",
    "yamljs": "^0.2.8"
  }
}
