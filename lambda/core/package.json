{
  "name": "tbd",
  "version": "0.0.1",
  "main": "handler.js",
  "author": "Jimmy Cann",
  "scripts": {
    "test": "NODE_ENV=integration-tests mocha --compilers js:babel-core/register ./**/__test__/bootstrap.js ./**/__test__/**/*.test.js",
    "init": "bash ./bin/initialize.sh",
    "deploy:all": "bash ./bin/init-deploy.sh $ENVIRONMENT",
    "deploy:all:test": "bash ./bin/init-deploy.sh test",
    "deploy:all:staging": "bash ./bin/init-deploy.sh staging",
    "deploy:all:live": "bash ./bin/init-deploy.sh live",
    "deploy:f": "bash ./bin/init-deploy.sh $ENVIRONMENT $FUNCTION",
    "deploy:f:test": "bash ./bin/init-deploy.sh test $FUNCTION",
    "deploy:f:staging": "bash ./bin/init-deploy.sh staging $FUNCTION ",
    "deploy:f:live": "bash ./bin/init-deploy.sh live $FUNCTION ",
    "invoke": "serverless invoke local --env development -f $FUNCTION -p ./.data/sample.json",
    "invoke:local": "serverless invoke local --env development -f $FUNCTION -p ./.data/sample.json",
    "invoke:test": "serverless invoke --profile nicbot-sls --env test -f $FUNCTION -p ./.data/sample.json",
    "invoke:staging": "serverless invoke --profile nicbot-sls --env staging -f $FUNCTION -p ./.data/sample.json",
    "invoke:live": "serverless invoke --profile nicbot-sls --env live -f $FUNCTION -p ./.data/sample.json",
    "gen-credentials": "AWS_PROFILE=nicbot-kms node ./bin/gen-credentials.js"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "dynasty": "^0.2.7"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-spies": "^0.7.1",
    "faker": "^4.1.0",
    "mocha": "^3.4.2",
    "nodecredstash": "^1.1.0",
    "serverless-babel-plugin": "^0.2.0",
    "sinon": "^2.3.2",
    "supertest": "^3.0.0",
    "yamljs": "^0.2.10"
  }
}
